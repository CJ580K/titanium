#!/usr/bin/env python
import os, os.path as path

Import("tiBuild")
Import('build')

tiBuild.add_thirdparty(build.env, 'poco')

env = tiBuild.env.Clone();
env.Append(CPPDEFINES = {'TITANIUM_NETWORK_API_EXPORT' : 1})

s = env.SharedLibrary(target = tiBuild.dir + '/tinetworkmodule', source = Glob('*.cpp'))

