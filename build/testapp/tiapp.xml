<?xml version="1.0" encoding="UTF-8"?>
<ti:app xmlns:ti="http://ti.appcelerator.org" xmlns:appc="http://www.appcelerator.org">
   <id>com.titaniumapp.test</id>
   <name>Titanium Test App</name>
   <version>0.1</version>
   <window>
      <id>initial</id>
      <title>Titanium Test Application</title>
      <url>app://index.html</url>
      <width>750</width>
      <height>850</height>
      <fullscreen>false</fullscreen>
      <minimizable>true</minimizable>
      <maximizable>true</maximizable>
      <closeable>true</closeable>
      <chrome>true</chrome>
   </window>
   <window>
      <id>smallscreen</id>
      <title>Titanium Smallscreen Window</title>
      <urlRegex>app://small.*.html</urlRegex>
      <width>400</width>
      <height>275</height>
      <minimizable>true</minimizable>
      <maximizable>false</maximizable>
      <closeable>true</closeable>
      <fullscreen>false</fullscreen>
      <resizable>false</resizable>
      <chrome>true</chrome>
   </window>
</ti:app>
